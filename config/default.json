{
    "encoders": {
        "input_dimensions": 3,
        "number_of_frequencies": 10,
        "frequencies_log_space": true,
        "use_viewdirs": true,
        "number_of_frequency_views": 4
    },
    "stratified_sampling": {
        "number_of_samples": 64,
        "perturb": true,
        "inverse_depth": false
    },
    "model": {
        "filter_dimensions": 128,
        "number_of_layers": 2,
        "skip": [],
        "use_fine_model": true,
        "fine_filter_dimensions": 128,
        "number_of_fine_layers": 6
    },
    "hierarchical_sampling": {
        "number_of_hierarchical_samples": 64,
        "perturb_hierarchical": false
    },
    "optimizer": {
        "learning_rate": 5e-4
    },
    "training": {
        "number_of_samples": 100,
        "test_image_idx": 101,
        "near": 2.0,
        "far": 6.0,
        "number_of_iterations": 10000,
        "batch_size": 65536,
        "one_image_per_step": true,
        "chunk_size": 65536,
        "center_crop": true,
        "center_crop_iterations": 50,
        "display_rate": 25
    },
    "early_stopping": {
        "warmup_iterations": 100,
        "warmup_min_fitness": 10.0,
        "number_of_restarts": 10
    }
}